<!DOCTYPE html>
<html lang="en">
    <head>
		<% title="About Us" %>
        <% include ../partials/head %>
    </head>

    <body>

        <% include ../partials/header %>

        <!-- Page Content -->
        <div class="container">
			<div class="jumbotron">
				<h2>Users</h2>
				<p>Registered Users:</p>
			</div>

			<div class="panel-group" id="user-panels">
				<% for(var i=0; i < users.length; i++){ %>
					<div class="panel panel-default">
						<div class="panel-heading">
							<h3 class="panel-title">
								<a href="/user/<%= users[i].id %>"><%= users[i].username %></a>
							</h3>
						</div>
						<div class="panel">
							<div class="panel-body">
								<% var imgSrc = users[i].profile.image ? users[i].profile.image : "http://robohash.org/" + users[i].id; %>
								<img src="<%= imgSrc %>" width="100" height="100" />
								<p>Id: <%= users[i].id %></p>
								<p>Email: <%= users[i].email %></p>
								<p>P: <%= users[i] %></p>
							</div>
						</div>
					</div>
				<% } %>

				<!-- TEMPLATE FOR PANELS
				<div class="panel panel-default">
					<div class="panel-heading">
						<h3 class="panel-title">
							<a data-toggle="collapse" data-parent="#about-panels" href="#MY_ID">TITLE HERE</a>
						</h3>
					</div>
					<div id="MY_ID" class="panel-collapse collapse">
						<div class="panel-body">

						</div>
					</div>
				</div>
				-->

			</div>

        </div>
        <!-- /.container -->

        <% include ../partials/footer %>

		<script>
			var id = window.location.hash.substr(1);
			$("#" +id).collapse("show"); // If they were given the URL (e.g. /about#security ) then show the panel
		</script>
        <!-- /.container -->
    </body>
</html>
